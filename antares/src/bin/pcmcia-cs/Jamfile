SubDir ANTARES_TOP src bin pcmcia-cs ;

SetSubDirSupportedPlatformsBeOSCompatible ;

if ! $(TARGET_PLATFORM_ANTARES_COMPATIBLE) {
	UseHeaders [ FDirName $(ANTARES_TOP) headers os drivers ] : true ;
		# We need the public pcmcia headers also when not compiling for Antares.
	UseHeaders [ FDirName $(ANTARES_TOP) headers os support ] : true ;
		# We need Errors.h also when not compiling for Antares.
}

StdBinCommands 
	cardctl.c
	dump_cis.c
	dump_cisreg.c
	: root ;

BinCommand pack_cis :
	pack_cis.c
	lex_cis.l
	yacc_cis.y
	;

	

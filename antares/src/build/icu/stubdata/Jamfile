SubDir ANTARES_TOP src tools icu stubdata ;

SEARCH_SOURCE += [ FDirName $(ANTARES_TOP) src libs icu source stubdata ] ;

UseLibraryHeaders icu ;

SubDirHdrs [ FDirName $(ANTARES_TOP) src libs icu source common ] ;

MakeLocate <build!stub>libicu-data.so : $(HOST_BUILD_COMPATIBILITY_LIB_DIR) ;

BuildPlatformSharedLibrary <build!stub>libicu-data.so :	
	stubdata.c
	;

SubDir ANTARES_TOP src add-ons kernel drivers input wacom ;

SetSubDirSupportedPlatformsBeOSCompatible ;

SubDirSysHdrs $(ANTARES_TOP) headers os drivers ;

KernelAddon wacom :
	wacom.c
;

Package antares-inputkit-cvs :
		wacpm :
		boot home config add-ons kernel drivers bin ;

PackageDriverSymLink antares-inputkit-cvs : input wacom ;


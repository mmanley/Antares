SubDir ANTARES_TOP src add-ons kernel drivers audio sb16 ;

SetSubDirSupportedPlatformsBeOSCompatible ;

UsePrivateHeaders media ;

KernelAddon sb16 :
	driver.c
	hooks.c
	sb16_hw.c
	sb16_multi_audio.c
;

Package antares-sb16-cvs :
	sb16 :
	boot home config add-ons kernel drivers bin ;
PackageDriverSymLink antares-sb16-cvs : audio multi sb16 ;
